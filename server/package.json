// server/package.json
{
  "name": "ryne-server",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "node --watch --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "start:ci": "NODE_ENV=ci node dist/index.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.13.0",
    "express": "^4.19.2",
    "firebase-admin": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "typescript": "^5.5.4"
  }
}
